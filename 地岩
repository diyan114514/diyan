q = char:GetChildren()
        for u = 1, #q do
        if q[u].ClassName == "Accessory" or q[u].ClassName == "Hat" then
                q[u]:remove()
                elseif q[u].ClassName == "CharacterMesh" then
                q[u]:remove()
            elseif q[u].ClassName == "ShirtGraphic" then
                q[u]:remove()
    elseif q[u].ClassName == "Shirt" then
        q[u]:Destroy()
elseif q[u].ClassName == "Pants" then
    q[u]:Destroy()
    end
end
